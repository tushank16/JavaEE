<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>RegisterServ</servlet-name>
  	<display-name>RegisterServ</display-name>
  	<description></description>
  	<servlet-class>RegisterServ</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ViewServ</servlet-name>
  	<display-name>ViewServ</display-name>
  	<description></description>
  	<servlet-class>mypack.ViewServ</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ViewServ1</servlet-name>
  	<display-name>ViewServ1</display-name>
  	<description></description>
  	<servlet-class>ViewServ1</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>RegisterServ</servlet-name>
  	<url-pattern>/RegisterServ</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ViewServ</servlet-name>
  	<url-pattern>/ViewServ</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ViewServ1</servlet-name>
  	<url-pattern>/ViewServ1</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
<welcome-file>Home.jsp</welcome-file>
</welcome-file-list>
</web-app>
